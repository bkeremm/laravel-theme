{
    "name": "bkeremm/laravel-theme",
    "description": "This package creates multiple managed theme infrastructure for Laravel.\n",
    "keywords": [
        "laravel",
        "theme",
        "laravel-theme",
        "laravel theme generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kerem Bahcivan",
            "email": "kerembahcivan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cankod\\Theme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cankod\\Theme\\ServiceProvider"
            ],
            "aliases": {
                "Theme": "Cankod\\Theme\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.3"
    },
    "require-dev": {
        "laravel/framework": ">=5.3"
    }
}
